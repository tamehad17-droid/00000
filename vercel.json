{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "headers": [
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!manifest\\.json|favicon\\.ico|logo\\.png|robots\\.txt|assets).*)",
      "destination": "/index.html"
    }
  ]
}
